{
    "name": "hc_api",
    "version": "1.0.0",
    "description": "HomeController backend API",
    "author": "Josh Dittmer",
    "private": true,
    "license": "ISC",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write .",
        "start:dev": "dotenvx run -f .env.dev -- nest start --watch",
        "start:debug": "dotenvx run -f .env.dev -- nest start --debug --watch",
        "start:prod": "dotenvx run -f .env.prod -- node dist/src/main.js",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "db:migrate": "drizzle-kit generate --name=init",
        "db:push": "drizzle-kit push",
        "db:seed": "dotenvx run -f .env.dev -- node --loader ts-node/esm drizzle/seed.ts"
    },
    "type": "module",
    "dependencies": {
        "@dotenvx/dotenvx": "^1.34.0",
        "@nestjs/common": "^11.0.11",
        "@nestjs/core": "^11.0.11",
        "@nestjs/platform-express": "^11.0.11",
        "@nestjs/platform-socket.io": "^11.0.11",
        "@nestjs/websockets": "^11.0.11",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.40.0",
        "io-ts": "^2.2.22",
        "io-ts-types": "^0.5.19",
        "jsonwebtoken": "^9.0.2",
        "npm-check-updates": "^17.1.15",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.5",
        "@nestjs/schematics": "^11.0.1",
        "@nestjs/testing": "^11.0.11",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20.17.11",
        "@types/pg": "^8.11.10",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "drizzle-kit": "^0.30.5",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.2",
        "eslint-plugin-prettier": "^5.2.3",
        "prettier": "^3.5.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}